//Linear Traversal Algorithm (O(n))
void resetFlood() {
        for (int i = 0; i < flooded.size(); i++)
            flooded[i] = false;
    }

//Boolean Flag array
void markFlooded(int node) {
    flooded[node] = true;
}

//Linear Loop 
void displayFloodStatus(const vector<string>& names)

//Input-Validation loop
int readInt(const string &prompt)

