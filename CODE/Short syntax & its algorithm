unordered_map<string, int> nameToNode;

//Sorting Algorithm 
sort(ranked.begin(), ranked.end());

//Queue 
queue<string> alertQueue;

//also a Queue but clearing traversal
while (!alertQueue.empty()) alertQueue.pop();
